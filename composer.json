{
	"name": "nette/nette",
	"type": "library",
	"description": "Nette Framework - innovative framework for fast and easy development of secured web applications in PHP. Write less, have cleaner code and your work will bring you joy.",
	"keywords": ["framework", "mvc", "debugging", "templating", "database", "forms", "mailing"],
	"homepage": "http://nette.org",
	"license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "David Grudl",
			"homepage": "http://davidgrudl.com"
		},
		{
			"name": "Nette Community",
			"homepage": "http://nette.org/contributors"
		}
	],
	"require": {
		"nette/application": ">=2.3.0",
		"nette/bootstrap": ">=2.3.0",
		"nette/caching": ">=2.3.0",
		"nette/component-model": ">=2.3.0",
		"nette/database": ">=2.3.0",
		"nette/di": ">=2.3.0",
		"nette/finder": ">=2.3.0",
		"nette/forms": ">=2.3.0",
		"nette/http": ">=2.3.0",
		"nette/mail": ">=2.3.0",
		"nette/neon": ">=2.3.0",
		"nette/php-generator": ">=2.3.0",
		"nette/reflection": ">=2.3.0",
		"nette/robot-loader": ">=2.3.0",
		"nette/security": ">=2.3.0",
		"nette/utils": ">=2.3.0",
		"latte/latte": ">=2.3.0",
		"tracy/tracy": ">=2.3.0"
	},
	"require-dev": {
		"nette/tester": "~1.3"
	},
	"autoload": {
		"classmap": ["Nette/"]
	},
	"minimum-stability": "dev",
	"extra": {
		"branch-alias": {
			"dev-master": "2.3-dev"
		}
	}
}
